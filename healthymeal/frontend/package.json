{
  "name": "healthymeal-frontend",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@chakra-ui/react": "^2.8.2",
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@mui/material": "^7.1.1",
    "@sentry/react": "^7.80.1",
    "@sentry/tracing": "^7.80.1",
    "@supabase/supabase-js": "^2.39.3",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^1.6.5",
    "formik": "^2.4.5",
    "framer-motion": "^10.18.0",
    "react": "^18.2.0",
    "react-bootstrap": "^2.10.10",
    "react-dom": "^18.2.0",
    "react-google-recaptcha": "^3.1.0",
    "react-icons": "^4.12.0",
    "react-router-dom": "^6.21.3",
    "react-scripts": "5.0.1",
    "react-toastify": "^9.1.3",
    "web-vitals": "^2.1.4",
    "yup": "^1.3.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "dev": "cross-env CI=true react-scripts test --passWithNoTests && react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:ci": "cross-env CI=true react-scripts test --passWithNoTests",
    "eject": "react-scripts eject",
    "setup:env": "node scripts/setup-env-test.js",
    "setup:env:test": "node -e \"require('./scripts/setup-env-test.js').setupUnitTestEnv()\"",
    "setup:env:e2e": "node -e \"require('./scripts/setup-env-test.js').setupE2ETestEnv()\"",
    "test:e2e": "npm run setup:env:e2e && npx playwright test",
    "test:e2e:ui": "npm run setup:env:e2e && npx playwright test --ui",
    "dev:test": "cross-env REACT_APP_TEST_MODE=true REACT_APP_USE_MOCKS=true react-scripts start",
    "dev:mock": "cross-env REACT_APP_USE_MOCKS=true react-scripts start"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "valid-jsdoc": [
        "error",
        {
          "requireReturn": true,
          "requireParamDescription": true,
          "requireReturnDescription": true
        }
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "jsdoc": "^4.0.2"
  }
}
