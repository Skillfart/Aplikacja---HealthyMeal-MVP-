{
  "name": "healthymeal-backend",
  "version": "1.0.0",
  "description": "Backend dla aplikacji HealthyMeal",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "cross-env NODE_ENV=development nodemon src/index.js",
    "dev:test": "npm run test:quick && nodemon src/server.js",
    "test": "jest",
    "test:e2e": "mocha src/tests/e2e/**/*.test.js",
    "test:unit": "mocha src/tests/unit/**/*.test.js",
    "test:integration": "mocha src/tests/integration/**/*.test.js",
    "test:quick": "mocha --reporter=min src/tests/unit/**/*.test.js",
    "test:openrouter": "node src/scripts/test-openrouter.js",
    "lint": "eslint src/",
    "dev:all": "npm run test:quick && echo \"Uruchamianie backendu na porcie 3030. OtwÃ³rz nowe okno terminala i uruchom: cd ../frontend && npm start\" && nodemon src/server.js",
    "prestart": "npm run test:quick",
    "start:test": "cross-env NODE_ENV=test node src/server.js",
    "dev:test-env": "cross-env NODE_ENV=test nodemon src/server.js",
    "dev:mock": "cross-env NODE_ENV=test USE_MOCKS=true nodemon src/server.js",
    "migrate": "migrate-mongo up",
    "migrate:down": "migrate-mongo down",
    "migrate:status": "migrate-mongo status",
    "migrate:create": "migrate-mongo create",
    "format": "prettier --write \"src/**/*.js\""
  },
  "dependencies": {
    "@openrouter/ai-sdk-provider": "^0.7.1",
    "@supabase/supabase-js": "^2.39.3",
    "axios": "^1.9.0",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.2.1",
    "helmet": "^7.1.0",
    "joi": "^17.12.1",
    "jsonwebtoken": "^9.0.2",
    "migrate-mongo": "^11.0.0",
    "mongoose": "^8.1.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-cache": "^5.1.2",
    "openai": "^4.24.0",
    "uuid": "^11.1.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "chai": "^4.3.10",
    "chai-http": "^4.4.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "mocha": "^10.2.0",
    "nodemon": "^3.0.3",
    "prettier": "^3.2.4",
    "supertest": "^6.3.4"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "author": "",
  "license": "ISC"
}
